            <Button Background="Blue" Height="35" Margin="20,20,20,0"/>
            <Button Background="Blue" Height="35" Margin="20,20,20,0"/>
            <Button Background="Blue" Height="35" Margin="20,20,20,0"/>
            
        </StackPanel>
    </Grid>
</Page>
