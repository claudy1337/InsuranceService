<Page x:Class="Insurance_Service.Pages.ClientAddPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Insurance_Service.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ClientAddPage" Background="White"
      xmlns:material="http://materialdesigninxaml.net/winfx/xaml/themes">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Width="300" Height="250" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox Height="40"/>
            <TextBox Height="40"/>
        </StackPanel>
        <StackPanel Grid.Column="1" HorizontalAlignment="Center" Width="300" Height="250" VerticalAlignment="Center">
            <TextBox Height="40" x:Name="TBName"/>
            <TextBox Height="40" x:Name="TBLastName"/>
            <TextBox Height="40" x:Name="TBFullName"/>
            <TextBox Height="40"/>
            <Button Height="30"/>
        </StackPanel>
    </Grid>
</Page>
